<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Country</th>
        <th>League</th>

        <th></th>
      </tr>
    </thead>
    <tbody>
      <% teams.forEach(function(t) { %>
        <tr>
          <td><%= t.name %></td>
          <td><%= t.country %></td>
          <td><%=t.league%></td>
          <td><a href="/teams/<%= t._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
</body>
</html>    



 
<%- include('../partials/footer') %>